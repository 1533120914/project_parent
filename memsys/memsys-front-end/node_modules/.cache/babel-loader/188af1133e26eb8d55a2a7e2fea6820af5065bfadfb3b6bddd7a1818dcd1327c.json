\r\n  },\r\n  created() {\r\n    this.providerId = this.$route.params.id\r\n    this.account = JSON.parse(localStorage.getItem('account'))\r\n    this.getProviderDetail()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AAiGA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,UAAA;MACAC,QAAA;MACAC,UAAA;IACA;EACA;EACAC,OAAA;IACAC,kBAAA;MACA,KAAAC,KAAA,CAAAC,GAAA,4BAAAN,UAAA,EACAO,IAAA,CAAAT,IAAA;QACA,KAAAG,QAAA,GAAAH,IAAA,CAAAG,QAAA;QACA,KAAAC,UAAA,GAAAJ,IAAA,CAAAU,IAAA;MACA;IACA;IACAC,kBAAAC,GAAA;MACA,KAAAL,KAAA,CAAAC,GAAA;QACAK,MAAA;UACAC,EAAA,EAAAF,GAAA,CAAAG,QAAA;UACAC,OAAA,OAAAf,OAAA,CAAAa;QACA;MACA,GAAAL,IAAA,CAAAT,IAAA;QACA,IAAAA,IAAA;UACA,KAAAiB,OAAA,CAAAC,IAAA;YACAnB,IAAA;YACAc,MAAA,EAAAD;UACA;QACA;UACA,KAAAO,QAAA,CAAAC,KAAA;QACA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAnB,UAAA,QAAAoB,MAAA,CAAAT,MAAA,CAAAC,EAAA;IACA,KAAAb,OAAA,GAAAsB,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA,KAAApB,iBAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}