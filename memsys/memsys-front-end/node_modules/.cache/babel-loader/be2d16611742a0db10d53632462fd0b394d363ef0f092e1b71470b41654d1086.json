(valid) => {\r\n        if (valid) {\r\n          if( this.formData.buyerId ) {\r\n            this.userList.forEach((user) => {\r\n              if(user.id == this.formData.buyerId) {\r\n                this.formData.buyerName = user.username\r\n              }\r\n            })\r\n          }\r\n          if( this.formData.managerId ) {\r\n            this.userList.forEach((user) => {\r\n              if(user.id == this.formData.managerId) {\r\n                this.formData.managerName = user.username\r\n              }\r\n            })\r\n          }\r\n          this.$http.post('/device-type/save', this.formData).then((data) => {\r\n            this.$router.push('/device-type')\r\n          })\r\n        }\r\n      })\r\n    }\r\n  },\r\n  created() {\r\n    this.deviceTypeId = this.$route.params.id\r\n    this.getDeviceType()\r\n    this.getUserList()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AA6BA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,YAAA;MACAC,QAAA;QACAJ,IAAA;MACA;MACAK,KAAA;QACAL,IAAA,GACA;UAAAM,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,OAAA;IACAC,YAAA;MACA,KAAAC,KAAA,CAAAC,GAAA,kBACAC,IAAA,CAAAZ,IAAA;QACA,KAAAC,QAAA,GAAAD,IAAA;MACA;IACA;IACAa,cAAA;MACA,KAAAH,KAAA,CAAAC,GAAA,+BAAAT,YAAA,EACAU,IAAA,CAAAZ,IAAA;QACA,KAAAG,QAAA,GAAAH,IAAA;MACA;IACA;IACAc,OAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,SAAAf,QAAA,CAAAgB,OAAA;YACA,KAAAlB,QAAA,CAAAmB,OAAA,CAAAC,IAAA;cACA,IAAAA,IAAA,CAAAC,EAAA,SAAAnB,QAAA,CAAAgB,OAAA;gBACA,KAAAhB,QAAA,CAAAoB,SAAA,GAAAF,IAAA,CAAAG,QAAA;cACA;YACA;UACA;UACA,SAAArB,QAAA,CAAAsB,SAAA;YACA,KAAAxB,QAAA,CAAAmB,OAAA,CAAAC,IAAA;cACA,IAAAA,IAAA,CAAAC,EAAA,SAAAnB,QAAA,CAAAsB,SAAA;gBACA,KAAAtB,QAAA,CAAAuB,WAAA,GAAAL,IAAA,CAAAG,QAAA;cACA;YACA;UACA;UACA,KAAAd,KAAA,CAAAiB,IAAA,2BAAAxB,QAAA,EAAAS,IAAA,CAAAZ,IAAA;YACA,KAAA4B,OAAA,CAAAC,IAAA;UACA;QACA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAA5B,YAAA,QAAA6B,MAAA,CAAAC,MAAA,CAAAV,EAAA;IACA,KAAAT,aAAA;IACA,KAAAJ,WAAA;EACA;AA