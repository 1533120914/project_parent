{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"LoginView\",\n  data() {\n    return {\n      account: {\n        username: null,\n        password: null,\n        identity: 1\n      },\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入登录名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    submitForm(formName) {\n      // 根据传过来的form名称寻找这个form,然后调用validate函数进行校验 (根据rules)\n      // valid是校验结果\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          // 执行登录\n          this.$http.post('/account/login', this.account).then(data => {\n            //data就是返回的account对象\n            // 存储当前用户到localStorage\n            localStorage.setItem('account', JSON.stringify(data));\n            // 跳转到主页\n            this.$router.push('/home');\n          });\n        } else {\n          // 不执行\n          return false;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","account","username","password","identity","rules","required","message","trigger","methods","submitForm","formName","$refs","validate","valid","$http","post","then","localStorage","setItem","JSON","stringify","$router","push"],"sources":["src/views/LoginView.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <el-card class=\"box-card\">\r\n      <div class=\"login-body\">\r\n        <div class=\"login-title\">医疗设备后台管理系统</div>\r\n        <el-form ref=\"form\" :model=\"account\" :rules=\"rules\">\r\n          <el-form-item prop=\"username\">\r\n            <el-input placeholder=\"请输入登录名...\" v-model=\"account.username\">\r\n              <template slot=\"prepend\">\r\n                <div class=\"el-icon-user-solid\"></div>\r\n              </template>\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <el-input placeholder=\"请输入密码...\" v-model=\"account.password\"\r\n                      @keyup.enter.native=\"submitForm('form')\"  show-password>\r\n              <template slot=\"prepend\">\r\n                <div class=\"el-icon-lock\"></div>\r\n              </template>\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-radio-group v-model=\"account.identity\" style=\"display: flex;justify-content: center;margin-bottom: 20px\">\r\n              <el-radio :label=\"1\">管理员</el-radio>\r\n              <el-radio :label=\"2\">员工</el-radio>\r\n            </el-radio-group>\r\n          </el-form-item>\r\n          <div class=\"login-submit\">\r\n            <el-button type=\"primary\" @click=\"submitForm('form')\">登录</el-button>\r\n          </div>\r\n        </el-form>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"LoginView\",\r\n  data() {\r\n    return {\r\n      account: {\r\n        username: null,\r\n        password: null,\r\n        identity: 1\r\n      },\r\n      rules: {\r\n        username: [\r\n          { required:true, message:'请输入登录名', trigger:'blur'}\r\n        ],\r\n        password: [\r\n          { required:true, message:'请输入密码', trigger:'blur'}\r\n        ]\r\n      }\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    submitForm(formName) {\r\n      // 根据传过来的form名称寻找这个form,然后调用validate函数进行校验 (根据rules)\r\n      // valid是校验结果\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          // 执行登录\r\n          this.$http.post('/account/login', this.account)\r\n          .then((data) => {  //data就是返回的account对象\r\n            // 存储当前用户到localStorage\r\n            localStorage.setItem('account', JSON.stringify(data))\r\n            // 跳转到主页\r\n            this.$router.push('/home')\r\n          })\r\n        } else {\r\n          // 不执行\r\n          return false;\r\n        }\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  width: 100%;\r\n  background-color: #f1f1f1;\r\n}\r\n\r\n.login-body {\r\n  padding: 30px;\r\n  width: 400px;\r\n  height: 100%;\r\n}\r\n\r\n.login-title {\r\n  padding-bottom: 30px;\r\n  text-align: center;\r\n  font-weight: 600;\r\n  font-size: 20px;\r\n  color: #409EFF;\r\n  cursor: pointer;\r\n}\r\n\r\n\r\n.login-submit {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.sign-in-container {\r\n  padding: 0 10px;\r\n}\r\n\r\n.sign-in-text {\r\n  color: #409EFF;\r\n  font-size: 16px;\r\n  text-decoration: none;\r\n  line-height:28px;\r\n}\r\n.other-submit{\r\n  display:flex;\r\n  justify-content: space-between;\r\n  margin-top: 10px;\r\n}\r\n</style>"],"mappings":";AAqCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;QACAC,QAAA;QACAC,QAAA;QACAC,QAAA;MACA;MACAC,KAAA;QACAH,QAAA,GACA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAL,QAAA,GACA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EAEAC,OAAA;IACAC,WAAAC,QAAA;MACA;MACA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA;UACA,KAAAC,KAAA,CAAAC,IAAA,wBAAAf,OAAA,EACAgB,IAAA,CAAAjB,IAAA;YAAA;YACA;YACAkB,YAAA,CAAAC,OAAA,YAAAC,IAAA,CAAAC,SAAA,CAAArB,IAAA;YACA;YACA,KAAAsB,OAAA,CAAAC,IAAA;UACA;QACA;UACA;UACA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}